{
  "entrypoint": "index.html",
  "shell": "src/cer-app.html",
  "fragments": [
    "src/cer-404.html",
    "src/painel/cer-painel.html",
    "src/usuarios/cer-usuarios-adicionar.html",
    "src/usuarios/cer-usuarios-editar.html",
    "src/usuarios/cer-usuarios-listar.html",
    "src/usuarios/cer-usuarios.html"
  ],
  "sourceGlobs": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "includeDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [{
    "addServiceWorker": false,
    "js": { "minify": false },
    "css": { "minify": false },
    "html": { "minify": false }
  }]
}
